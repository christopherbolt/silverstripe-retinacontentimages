{
    "name": "christopherbolt/silverstripe-retinacontentimages",
    "type": "silverstripe-vendormodule",
    "description": "Adds retina image support to images added through the HTMLEditorField.",
    "homepage": "https://github.com/christopherbolt/silverstripe-retinacontentimages",
    "authors": [
        {
            "name": "Chris Bolt",
            "email": "chris@christopherbolt.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "silverstripe/cms": "^4.0",
    	"silverstripe/framework": "^4.0"
    },
	"extra": {
        "installer-name": "retinacontentimages",
    },
	"autoload": {
        "psr-4": {
            "ChristopherBolt\\RetinaContentImages\\": "src/"
        }
    }
}
